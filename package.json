    "eslint-config-euclids-node": "0.1.1",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-mocha": "9.0.0",
    "mocha": "9.0.3",
    "sinon": "11.1.2"
  },
  "dependencies": {}
